{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "arkts",
      "camelcase": "Arkts",
      "title": "Arkts",
      "scope": "source.arkts",
      "file-types": [
        "ets"
      ],
      "injection-regex": "^arkts$",
      "class-name": "TreeSitterArkts"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "paser for arkts",
    "authors": [
      {
        "name": "million"
      }
    ],
    "links": {
      "repository": "https://github.com/Million-mo/arkts_language_server"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}